<%- include('base.ejs') %>
 <link rel="stylesheet" href="/css/home.css">
 <link rel="stylesheet" href="/css/forms.css">
 <link rel="stylesheet" href="/css/product.css">
</head>

<body>

<%- include('nav.ejs') %>

<main>
    <div class="grid">
        <% for (var i = 0; i < prof.length; i++) { %>
        <% var pro = prof[i]; %>
    <article class="card product-item">
                    <header class="card__header">
                        <h1 class="product__title"> Name: 
                            <%= pro.name_fn %>
                        </h1>
                    </header>
                    
                    <div class="card__content">
                        <h3 class="product__price"> Gender: 
                            <%= pro.gender %>
                        </h3>
                        <h3 class="product__description">Age: 
                            <%= pro.age %>
                        </h3>
						<h3 class="product__description">Experience: 
                            <%= pro.experience %>
                        </h3>
						<h3 class="product__description">Salary: 
                            <%= pro.salary %>
                        </h3>
						<h3 class="product__description">Role: 
                            <%= pro.role %>
                        </h3>
                    </div>
    </article>
        <% } %>
    </div>
</main>
</body>
